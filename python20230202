cycle = 0
N = int(input('0보다 크거나 같고 00보다 작은 정수 N을 입력해보세용: '))
original_N = N

while True:
    
    if N < 10:
        N = 10*N + N
        cycle += 1
    
    else:
        N1 = N // 10 #2
        N2 = N % 10 #6
        N3 = N1 + N2 #8
        N4 = N3 %10 #8
        N = N2*10 + N4
        cycle += 1

    if N == original_N:
        print(cycle) 
        break
